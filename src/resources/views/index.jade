extend layout
block content
  each s in searchResults.sets
    .set
      span.setName= s.setName
      img(src=s.thumbnailURL)
block facet
  facetName = "Theme"
  facetQuery = "theme"
  facet = searchResults.theme
  include facet

  facetName = "Year"
  facetQuery = "year"
  facet = searchResults.year
  include facet
